{
  "source": {
    "include": ["./src/"]
  },
  "templates": {
    "default": {
      "useLongnameInNav": true
    },
    "systemName": "Paratii",
    "outputSourceFiles": true,
    "search": true,
    "theme" : "flatly"
  },
  "opts": {
    "template": "node_modules/ink-docstrap/template",
    "tutorials": "./docs/tutorials",
    "readme": "./docs/JSDOC-INDEX.md",
    "destination": "./docs/htmldoc"
  },
 "plugins": ["plugins/markdown"]
}
