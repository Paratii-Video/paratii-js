{
  "name": "paratii-lib",
  "version": "0.0.1",
  "description": "Paratii Smart Contracts",
  "main": "dist/paratii.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "babel ./lib -d ./dist",
    "test": "mocha --require truffle.js test/*.js --exit",
    "testrpc": "testrpc --deterministic",
    "lint": "standard",
    "lint-fix": "standard --fix",
    "node": "node"
  },
  "dependencies": {
    "default-options": "^1.0.0",
    "ipfs": "github:ya7ya/js-ipfs#paratii/v0.27",
    "paratii-contracts": "github:Paratii-Video/paratii-contracts#master",
    "paratii-protocol": "github:ya7ya/paratii-protocol#master",
    "promisify-node": "^0.4.0",
    "pull-filereader": "^1.0.1",
    "stream-to-pull-stream": "^1.7.2",
    "vimeo-downloader": "^1.0.3",
    "web3": "^1.0.0-beta.24",
    "ytdl-core": "^0.18.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-async-functions": "^6.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "ethereumjs-testrpc": "^6.0.1",
    "mocha": "^4.0.1",
    "pre-commit": "^1.2.2",
    "standard": "^10.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Paratii-Video/paratii-lib"
  },
  "keywords": [
    "javascript",
    "ethereum",
    "library",
    "API",
    "paratii"
  ],
  "author": "Jelle Gerbrandy <jelle@gerbrandy.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Paratii-Video/paratii-lib/issues"
  },
  "homepage": "https://github.com/Paratii-Video/paratii-lib",
  "standard": {
    "envs": [
      "mocha"
    ],
    "globals": [
      "artifacts",
      "assert",
      "contract",
      "it",
      "web3"
    ],
    "ignore": [
      "dist"
    ]
  },
  "pre-commit": [
    "lint",
    "build"
  ]
