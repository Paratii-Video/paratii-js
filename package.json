{
  "name": "paratii-js",
  "version": "0.2.0",
  "description": "Paratii JS Library",
  "main": "dist/paratii.js",
  "browser": {
    "fs": false,
    "path": false,
    "net": false
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "babel ./src -d ./dist",
    "build-browser": "webpack --progress --colors",
    "test": "mocha --require babel.js --exit --timeout 20000 test/*.js",
    "parity": "parity --config parity-config.toml",
    "lint": "standard",
    "lint-fix": "standard --fix",
    "node": "node",
    "docs": "jsdoc --configure .jsdoc.json" ,
    "git-add-dist": "git add dist"
  },
  "dependencies": {
    "async": "^2.6.0",
    "bip39": "^2.5.0",
    "chai-bignumber": "^2.0.2",
    "es6-promise": "^4.2.4",
    "hdkey": "^0.8.0",
    "ipfs": "github:Paratii-Video/js-ipfs#f02bfb4961eaf8fae077eb12e175cd518e602bc4",
    "isomorphic-fetch": "^2.2.1",
    "joi": "^13.1.2",
    "multiaddr": "^3.0.2",
    "once": "^1.4.0",
    "paratii-contracts": "github:Paratii-Video/paratii-contracts#6651984d0bf7f2a905631087975c5a412d5e4cb7",
    "paratii-protocol": "github:Paratii-Video/paratii-protocol#45aadcf300f1ad184d6d3db8f9c4b3d438b6f658",
    "promisify-node": "^0.4.0",
    "pull-filereader": "^1.0.1",
    "resumablejs": "^1.1.0",
    "stream-to-pull-stream": "^1.7.2",
    "web3": "^1.0.0-beta.29"
  },
  "devDependencies": {
    "async_hooks": "^1.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-async-functions": "^6.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "browserify-fs": "^1.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "file-api": "^0.10.4",
    "ink-docstrap": "^1.3.2",
    "jest": "^22.4.3",
    "jsdoc": "^3.5.5",
    "mocha": "^4.1.0",
    "nock": "^9.1.6",
    "postinstall-prepare": "^1.0.1",
    "pre-commit": "^1.2.2",
    "standard": "^10.0.3",
    "webpack": "^3.11.0",
    "why-is-node-running": "^1.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Paratii-Video/paratii-js"
  },
  "keywords": [
    "javascript",
    "ethereum",
    "library",
    "API",
    "paratii"
  ],
  "author": "Paratii <hello@paratii.video>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Paratii-Video/paratii-js/issues"
  },
  "homepage": "https://github.com/Paratii-Video/paratii-js",
  "standard": {
    "envs": [
      "mocha"
    ],
    "parser": "babel-eslint",
    "globals": [
      "artifacts",
      "assert",
      "contract",
      "it",
      "web3"
    ],
    "ignore": [
      "dist",
      "docs"
    ]
  },
  "pre-commit": [
    "lint",
    "build",
    "build-browser",
    "docs",
    "git-add-dist"
  ]
}
